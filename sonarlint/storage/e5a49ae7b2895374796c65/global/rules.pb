let my_pwd;
my_pwd = "qwerewt";

login({ passwd: "zxvxcv"});

const url = "https://example.com?password=hl2OAIXXZ60";
</pre>
<h2>Compliant Solution</h2>
<pre>
const password = loadCredentials();
</pre>
